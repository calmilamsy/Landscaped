{
  "schemaVersion": 1,
  "id": "landscaped",
  "version": "${version}",

  "name": "Landscaped",
  "description": "A simple partial port of forestry to b1.7.3 StationAPI.",
  "authors": [
    "calmilamsy"
  ],
  "contact": {
    "homepage": "https://glass-launcher.net/",
    "sources": "https://github.com/calmilamsy/Forested"
  },

  "license": "LGPLV3",
  "icon": "assets/landscaped/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "net.glasslauncher.mods.landscaped.Landscaped",
      "net.glasslauncher.mods.landscaped.eventlisteners.init.BlockInit",
      "net.glasslauncher.mods.landscaped.eventlisteners.init.WoodTextureListener",
      "net.glasslauncher.mods.landscaped.eventlisteners.init.LeavesTextureListener",
      "net.glasslauncher.mods.landscaped.eventlisteners.init.TreeGenerationInit"
    ],
    "stationapi:event_bus_client": [
    ],
    "stationapi:event_bus_server": [
    ]
  },
  "mixins": [
    "landscaped.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
